---
import BaseLayout from '@layouts/BaseLayout.astro';
import ConsistencySection from '@components/ta-ConsistencySection.astro';
import Ambassadors from '@components/ta-AmbassadorGallery.astro';
import Footer from '@components/dv-Footer.astro';
import Header from '@components/Header.astro';
import Hero from '@components/ta-Hero.astro';
import CenteredSection from '@components/ta-CenteredSection.astro';
import PageContainer from '@components/PageContainer.astro';
import BecomeCTA from '@components/ta-BecomeCTA.astro';
import StillHaveQuestions from '@components/ta-StillHaveQuestions.astro';

import '@styles/global.css';
import '@styles/custom-page.css';
---

<BaseLayout>
  <Header showThemeToggle={false} />
  <Hero />

  <PageContainer>

    <div class="flex flex-col gap-12 md:gap-38 z-10">
      <CenteredSection />
      <ConsistencySection />
      <Ambassadors />
    </div>

    <div class="flex flex-col gap-44 z-10">
      <BecomeCTA />
      <StillHaveQuestions />
      <Footer />
    </div>
  </PageContainer>
</BaseLayout>
