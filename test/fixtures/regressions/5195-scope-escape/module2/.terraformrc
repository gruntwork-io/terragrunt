
provider_installation {

   "filesystem_mirror" {
    include = ["registry.terraform.io/*/*", "registry.opentofu.org/*/*"]
    path    = "/home/denis256/.cache/terragrunt/providers"
  }
   "direct" {
  }
}

plugin_cache_dir = ""

host "registry.terraform.io" {
  services = {
    "modules.v1"   = "https://registry.terraform.io/v1/modules/"
    "providers.v1" = "http://127.0.0.1:45553/v1/providers//registry.terraform.io/"
  }
}
host "registry.opentofu.org" {
  services = {
    "modules.v1"   = "https://registry.opentofu.org/v1/modules/"
    "providers.v1" = "http://127.0.0.1:45553/v1/providers//registry.opentofu.org/"
  }
}

disable_checkpoint           = false
disable_checkpoint_signature = false
